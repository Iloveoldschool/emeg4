		Vehicle *theCar = NULL;
		float bestCar = 0.;
		VehicleList patrolCars(VT_FIREFIGHTERS_TLF, VT_FIREFIGHTERS_TLF);
		for (int i = 0; i < patrolCars.GetNumVehicles(); i++)
		{
			Vehicle *aCar = patrolCars.GetVehicle(i);
